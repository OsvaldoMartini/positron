{
  "presets": [
    "es2015",
    "react",
    "stage-3"
  ],
  "plugins": [
    "babel-plugin-rewire",
    "inline-react-svg",
    "transform-runtime",
    ["module-resolver", {
      "root": ["./"]
    }],
    "transform-class-properties",
    ["module-alias", [
      {
        "src": "./node_modules/styled-components/dist/styled-components.js",
        "expose": "styled-components"
      }
    ]]
  ],
  "env": {
    "development": {
      "plugins": [
        ["styled-components", {
          "ssr": true
        }]
      ]
    }
  }
}
